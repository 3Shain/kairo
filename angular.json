{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "yarn"
  },
  "defaultProject": "ng-app",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "kairo": {
      "root": "libs/kairo",
      "sourceRoot": "libs/kairo/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/kairo/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/kairo"],
          "options": {
            "jestConfig": "libs/kairo/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "./tools/executors/rollup:package",
          "options": {
            "externals": ["tslib"],
            "bundleName": "kairo",
            "copy": []
          }
        }
      }
    },
    "ng-app": {
      "projectType": "application",
      "root": "apps/ng-app",
      "sourceRoot": "apps/ng-app/src",
      "prefix": "realkairo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ng-app",
            "index": "apps/ng-app/src/index.html",
            "main": "apps/ng-app/src/main.ts",
            "polyfills": "apps/ng-app/src/polyfills.ts",
            "tsConfig": "apps/ng-app/tsconfig.app.json",
            "assets": ["apps/ng-app/src/favicon.ico", "apps/ng-app/src/assets"],
            "styles": ["apps/ng-app/src/styles.scss"],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ng-app/src/environments/environment.ts",
                  "with": "apps/ng-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ng-app/src/**/*.ts",
              "apps/ng-app/src/**/*.html"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ng-app"],
          "options": {
            "jestConfig": "apps/ng-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ng-lib": {
      "projectType": "library",
      "root": "libs/ng-lib",
      "sourceRoot": "libs/ng-lib/src",
      "prefix": "kairo",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/ng-lib/tsconfig.lib.json",
            "project": "libs/ng-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ng-lib/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ng-lib/src/**/*.ts",
              "libs/ng-lib/src/**/*.html"
            ]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ng-lib"],
          "options": {
            "jestConfig": "libs/ng-lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "preact-lib": {
      "root": "libs/preact-lib",
      "sourceRoot": "libs/preact-lib/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "./tools/executors/rollup:package",
          "options": {
            "externals": ["kairo", "preact", "preact/compat"],
            "bundleName": "kairo-preact",
            "copy": []
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/preact-lib/**/*.{ts,tsx,js,jsx}"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/preact-lib"],
          "options": {
            "jestConfig": "libs/preact-lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-app": {
      "root": "apps/react-app",
      "sourceRoot": "apps/react-app/src",
      "projectType": "application",
      "architect": {
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx vite",
            "cwd": "apps/react-app"
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx vite build",
            "cwd": "apps/react-app"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/react-app/**/*.{ts,tsx,js,jsx}"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/react-app"],
          "options": {
            "jestConfig": "apps/react-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-lib": {
      "root": "libs/react-lib",
      "sourceRoot": "libs/react-lib/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "./tools/executors/rollup:package",
          "options": {
            "externals": ["kairo", "react"],
            "bundleName": "kairo-react",
            "copy": []
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/react-lib/**/*.{ts,tsx,js,jsx}"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/react-lib"],
          "options": {
            "jestConfig": "libs/react-lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "solid-app": {
      "root": "apps/solid-app",
      "sourceRoot": "apps/solid-app/src",
      "projectType": "application",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/solid-app/**/*.{ts,tsx,js,jsx}"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx vite",
            "cwd": "apps/solid-app"
          }
        }
      }
    },
    "solid-lib": {
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/solid-lib/**/*.{ts,tsx}"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "build": {
          "builder": "./tools/executors/rollup:package",
          "options": {
            "externals": ["kairo", "solid-js"],
            "bundleName": "kairo-solid",
            "copy": []
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/solid-lib"],
          "options": {
            "jestConfig": "libs/solid-lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "root": "libs/solid-lib",
      "sourceRoot": "libs/solid-lib/src"
    },
    "svelte-app": {
      "root": "apps/svelte-app",
      "sourceRoot": "apps/svelte-app/src",
      "projectType": "application",
      "architect": {
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx vite",
            "cwd": "apps/svelte-app"
          }
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx vite build",
            "cwd": "apps/svelte-app"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/svelte-app/**/*.{ts,tsx,js,jsx}"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/svelte-app"],
          "options": {
            "jestConfig": "apps/svelte-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "svelte-lib": {
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/svelte-lib/**/*.{ts,tsx}"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "build": {
          "builder": "./tools/executors/rollup:package",
          "options": {
            "externals": ["kairo", "svelte", "svelte/internal"],
            "bundleName": "kairo-svelte"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/svelte-lib"],
          "options": {
            "jestConfig": "libs/svelte-lib/jest.config.js",
            "passWithNoTests": true
          }
        },
        "test-ssr": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/svelte-lib"],
          "options": {
            "jestConfig": "libs/svelte-lib/jest.config.ssr.js",
            "passWithNoTests": true
          }
        }
      },
      "root": "libs/svelte-lib",
      "sourceRoot": "libs/svelte-lib/src"
    },
    "svelte-preprocess": {
      "root": "libs/svelte-preprocess",
      "sourceRoot": "libs/svelte-preprocess/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "./tools/executors/rollup:package",
          "options": {
            "externals": [""],
            "bundleName": "svelte-preprocess",
            "copy": []
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/svelte-preprocess/**/*.{ts,tsx,js,jsx}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      }
    },
    "vite-plugin-vue": {
      "root": "libs/vite-plugin-vue",
      "sourceRoot": "libs/vite-plugin-vue/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "./tools/executors/rollup:package",
          "options": {
            "externals": [""],
            "bundleName": "vite-plugin-vue",
            "copy": []
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/vite-plugin-vue/**/*.{ts,tsx,js,jsx}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      }
    },
    "vite-vue-app": {
      "root": "apps/vite-vue-app",
      "sourceRoot": "apps/vite-vue-app/src",
      "projectType": "application",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/solid-app/**/*.{ts,tsx,js,jsx}"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx vite build",
            "cwd": "apps/vite-vue-app"
          }
        },
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx vite",
            "cwd": "apps/vite-vue-app"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/vite-vue-app"],
          "options": {
            "jestConfig": "apps/vite-vue-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "vue-lib": {
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/vue-lib/**/*.{ts,tsx,vue}"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "build": {
          "builder": "./tools/executors/rollup:package",
          "options": {
            "externals": ["kairo", "vue", "@vue/reactivity"],
            "bundleName": "kairo-vue",
            "copy": ["./interop.d.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/vue-lib"],
          "options": {
            "jestConfig": "libs/vue-lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "root": "libs/vue-lib",
      "sourceRoot": "libs/vue-lib/src"
    },
    "vue-sfc-transformer": {
      "root": "libs/vue-sfc-transformer",
      "sourceRoot": "libs/vue-sfc-transformer/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "./tools/executors/rollup:package",
          "options": {
            "externals": [],
            "bundleName": "vue-sfc-transformer",
            "copy": []
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/vue-sfc-transformer/**/*.{ts,tsx,js,jsx}"
            ]
          },
          "outputs": ["{options.outputFile}"]
        }
      }
    }
  }
}
